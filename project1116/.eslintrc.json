{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-import-module-exports": "off",
    "no-console": "off",
    "no-restricted-globals": "off",
    "no-use-before-define": "off"
  }
}
